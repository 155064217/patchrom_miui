<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android" >
    <!-- Rearranged to use EditPinPreference -->
    <com.android.phone.MIUICallBarringBasePreference
        android:key="all_outing_key"
        android:persistent="false"
        android:summary="@string/lable_disable"
        android:title="@string/lable_all_outgoing" />
    <com.android.phone.MIUICallBarringBasePreference
        android:key="all_outing_international_key"
        android:persistent="false"
        android:summary="@string/lable_disable"
        android:title="@string/lable_all_outgoing_international" />
    <com.android.phone.MIUICallBarringBasePreference
        android:key="all_outing_except_key"
        android:persistent="false"
        android:summary="@string/lable_disable"
        android:title="@string/lable_all_out_except" />
    <com.android.phone.MIUICallBarringBasePreference
        android:key="all_incoming_key"
        android:persistent="false"
        android:summary="@string/lable_disable"
        android:title="@string/lable_all_incoming" />
    <com.android.phone.MIUICallBarringBasePreference
        android:key="all_incoming_except_key"
        android:persistent="false"
        android:summary="@string/lable_disable"
        android:title="@string/lable_incoming_calls_except" />
    <com.android.phone.MIUICallBarringResetPreference
        android:dialogMessage="@string/enter_pin2_text"
        android:key="deactivate_all_key"
        android:persistent="false"
        android:title="@string/deactivate_all" />
    <PreferenceScreen
        android:dialogTitle="@string/change_password"
        android:key="change_password_key"
        android:persistent="false"
        android:title="@string/change_password" />
</PreferenceScreen>
